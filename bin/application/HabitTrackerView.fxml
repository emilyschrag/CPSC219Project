<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="348.0" prefWidth="311.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainSceneController">
   <children>
      <Label text="My Habit Tracker">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" top="10.0" />
         </VBox.margin>
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Line endX="200.0" startX="-100.0" stroke="#000000ac">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" top="3.0" />
         </VBox.margin>
      </Line>
      <HBox prefHeight="25.0" prefWidth="460.0">
         <children>
            <Label text="Enter your Information:">
               <HBox.margin>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <Button mnemonicParsing="false" onAction="#enterInfo" text="Enter my Info">
               <HBox.margin>
                  <Insets bottom="5.0" right="10.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox prefHeight="16.0" prefWidth="460.0">
         <children>
            <Label text="Enter today's date:">
               <HBox.margin>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </HBox.margin>
            </Label>
            <DatePicker fx:id="todaysDatePicker">
               <HBox.margin>
                  <Insets bottom="5.0" right="10.0" />
               </HBox.margin>
            </DatePicker>
         </children>
      </HBox>
      <Line endX="200.0" startX="-100.0" stroke="#00000058">
         <VBox.margin>
            <Insets bottom="10.0" left="5.0" top="5.0" />
         </VBox.margin>
      </Line>
      <Label text="Habits">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" />
         </VBox.margin>
      </Label>
      <Button mnemonicParsing="false" onAction="#toSleep" text="Sleep">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="10.0" />
         </VBox.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#toFood" text="Food">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="10.0" />
         </VBox.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#toExpenses" text="Expenses">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="10.0" />
         </VBox.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#toExercise" text="Exercise">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="10.0" />
         </VBox.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#toWater" text="Water">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="10.0" />
         </VBox.margin>
      </Button>
      <Line endX="200.0" startX="-100.0" stroke="#00000078">
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </Line>
      <Label text="Daily Score Calculator">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" top="5.0" />
         </VBox.margin>
      </Label>
      <Button mnemonicParsing="false" onAction="#getDailyScore" text="Get My Daily Score!">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="10.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
